{
  "compilerOptions": {
    "isolatedModules": true,
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "bundler",
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    //"noUnusedParameters": true,
    "baseUrl": ".",
    "paths": {
      "@ha/*": ["/workspaces/frontend/src/*"]
    }
  }
}
